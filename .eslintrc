{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "rules": {
        "indent": ["error", 4],
        "react/sort-comp": 0,
        "react/prop-types": 0,
        "import/named": 0,
        "import/no-unresolved": 0,
        "import/prefer-default-export": 0
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    }
}